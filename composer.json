{
    "name": "etraxis/etraxis",
    "description": "Issue tracking system with customizable workflows.",
    "type": "project",
    "license": "GPL-3.0",
    "support": {
        "forum"  : "https://gitter.im/etraxis",
        "issues" : "https://github.com/etraxis/etraxis/issues",
        "wiki"   : "https://github.com/etraxis/etraxis/wiki",
        "source" : "https://github.com/etraxis/etraxis"
    },
    "require": {
        "php"                                  : ">=7.0",
        "symfony/symfony"                      : "3.3.*",
        "symfony/polyfill-apcu"                : "^1.0",
        "symfony/monolog-bundle"               : "^3.1",
        "symfony/swiftmailer-bundle"           : "^3.0",
        "sensio/framework-extra-bundle"        : "^3.0.2",
        "sensio/distribution-bundle"           : "^5.0.19",
        "incenteev/composer-parameter-handler" : "^2.0",
        "twig/twig"                            : "^2.0",
        "twig/extensions"                      : "^1.5",
        "doctrine/orm"                         : "^2.5",
        "doctrine/doctrine-bundle"             : "^1.6",
        "doctrine/doctrine-migrations-bundle"  : "^1.2",
        "doctrine/doctrine-fixtures-bundle"    : "^2.3",
        "league/tactician-bundle"              : "~0.4",
        "league/tactician-doctrine"            : "^1.1",
        "ramsey/uuid"                          : "^3.6",
        "webinarium/php-dictionary"            : "^1.1",
        "webinarium/php-properties"            : "^0.0.1",
        "webinarium/pignus-bundle"             : "^1.0",
        "roave/security-advisories"            : "dev-master"
    },
    "require-dev": {
        "symfony/phpunit-bridge"               : "^3.0",
        "sensio/generator-bundle"              : "^3.0",
        "composer/composer"                    : "^1.4",
        "friendsofphp/php-cs-fixer"            : "^2.3"
    },
    "autoload": {
        "psr-4": {
            "AppBundle\\" : "src/AppBundle",
            "eTraxis\\"   : "src/eTraxis"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AppBundle\\" : "tests/AppBundle",
            "eTraxis\\"   : "tests/eTraxis"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "eTraxis\\Composer\\ParameterHandler::updateSecret",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": false
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
